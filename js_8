function finalGrade (exam, project) {
let grade = [100, 90, 75, 0];
	if (exam > 90 || project > 10) {
		return grade[0];
	} else if ((exam > 75 && exam <= 90) || (project >= 5 && project <= 10)) {
		return grade[1];
	} else if ((exam > 50 && exam <= 75) || (project >= 2 && project < 5)) {
		return grade[2];
	} else if (exam <= 50 || project < 2) {
		return 0;
	}
};
